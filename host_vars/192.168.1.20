# Host specific
# These settings are unique to this particular host.

# Server settings for this host
unattended_upgrades: false
# Disables automatic security updates for this host.
# This might be set to false if you prefer to manage updates manually or if the server requires strict update control.

proxmox_server: true
# Enables the Proxmox server role on this host, indicating that this server is part of a Proxmox virtualization environment.
# This setting will trigger tasks related to managing Proxmox on this host.