write-kubeconfig-mode: "0644"
token: {{ hostvars['RKE2-server1']['token'] }}
server: https://{{ hostvars['RKE2-server1']['ansible_host'] }}:9345
tls-san:
  - {{ vip }}
  - {{ hostvars['RKE2-server1']['ansible_host'] }}
  - {{ hostvars['RKE2-server2']['ansible_host'] }}
  - {{ hostvars['RKE2-server3']['ansible_host'] }}
node-label:
  - server=true
disable:
  - rke2-ingress-nginx