# {{ ansible_managed }}
{% if ansible_distribution_major_version | int >= 12 %}
deb http://ftp.nl.debian.org/debian {{ ansible_distribution_release }} main contrib non-free non-free-firmware
deb http://ftp.nl.debian.org/debian {{ ansible_distribution_release }}-updates main contrib non-free non-free-firmware
deb http://ftp.nl.debian.org/debian-security {{ ansible_distribution_release }}-security main contrib non-free non-free-firmware
{% else %}
deb http://ftp.nl.debian.org/debian {{ ansible_distribution_release }} main contrib non-free
deb http://ftp.nl.debian.org/debian {{ ansible_distribution_release }}-updates main contrib non-free
deb http://ftp.nl.debian.org/debian-security {{ ansible_distribution_release }}-security main contrib non-free
{% endif %}
